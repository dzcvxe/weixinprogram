<!--components/searach/search.wxml-->
<view class="search">
  <navigator url="/pages/search_/search_"  open-type="navigate">搜索</navigator>
  <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
    <block wx:for="{{imgUrls}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image" />
      </swiper-item>
    </block>
  </swiper>
</view>
